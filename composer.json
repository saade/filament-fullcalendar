{
    "name": "saade/filament-fullcalendar",
    "description": "The Most Popular JavaScript Calendar integrated with Filament ðŸ’›",
    "keywords": [
        "saade",
        "laravel",
        "filament-fullcalendar"
    ],
    "homepage": "https://github.com/saade/filament-fullcalendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Saade",
            "email": "saade@outlook.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.0",
        "illuminate/contracts": "^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0|^7.0",
        "nunomaduro/larastan": "^1.0|^2.0",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Saade\\FilamentFullCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saade\\FilamentFullCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saade\\FilamentFullCalendar\\FilamentFullCalendarServiceProvider"
            ],
            "aliases": {
                "FilamentFullCalendar": "Saade\\FilamentFullCalendar\\Facades\\FilamentFullCalendar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
